<div class="section-carousel {{ section.class }}">
  Section carousel
  <div vue>
    <carousel>
      <div class="w-full slide">
        <img src="http://via.placeholder.com/640x360" width="640" height="360" loading="lazy" {% if section.index0 == 0 %} loading="eager"{% endif %}" />
      </div>
      <div class="w-full slide">Slide 2</div>
      <div class="w-full slide">
        <img src="http://via.placeholder.com/640x660" width="640" height="660" loading="lazy" {% if section.index0 == 0 %} loading="eager"{% endif %}" />
      </div>
      <div class="w-full slide">Slide 4</div>
    </carousel>
  </div>
</div>

{% schema %}
{
  "name": "Carousel",
  "class": "shopify-section-vue",
  "settings": [

  ],
  "presets": [
    {
      "name": "Carousel",
      "category": "Theme"
    }
  ]
}
{% endschema %}